{
  "name": "stateface",
  "version": "1.0.1",
  "description": "Stateface, an icon font library for the 50 US states, DC, and Puerto Rico.",
  "repository": {
    "type": "git",
    "url": "git://github.com/deardooley/stateface.git"
  },
  "main": [
    "font/webfont/stateface-regular-webfont.eot",
    "font/webfont/stateface-regular-webfont.svg",
    "font/webfont/stateface-regular-webfont.ttf",
    "font/webfont/stateface-regular-webfont.woff",
    "reference/stateface.css"
  ],
  "dependencies": { },
  "devDependencies": { },
  "ignore": [
    "**/.*",
    "**/*.txt",
    "**/*.md",
    "**/*.mdown",
    "**/*.rb",
    "**/*.json",
    "**/*.py",
    "**/*.html",
    "eps",
    "font/*.glyphs",
    "font/*.otf",
    "pkg",
    "reference/styles.css",
    "tools",
    "Makefile"
  ]
}
